<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sixteenth_activity"
    android:padding="5dp">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/backgroundiiii">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/titlepage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/bungee"
                android:text="ABS WORKOUT"
                android:textColor="@color/white"
                android:textSize="25dp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="178dp"
                android:layout_height="86dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/titlepage"
                app:srcCompat="@drawable/crunches" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="60dp"
                android:text="Crunches"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView"
                app:layout_constraintTop_toBottomOf="@id/titlepage" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="60dp"
                android:text="25 reps"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView"
                app:layout_constraintTop_toBottomOf="@id/textView" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="168dp"
                android:layout_height="92dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/imageView"
                android:layout_marginTop="10dp"
                app:srcCompat="@drawable/flutterkicks" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:paddingLeft="50dp"
                android:text="Flutter Kicks"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView2"
                app:layout_constraintTop_toBottomOf="@id/textView2" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="50dp"
                android:text="20 reps"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView2"
                app:layout_constraintTop_toBottomOf="@id/textView3" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="187dp"
                android:layout_height="105dp"
                android:layout_marginTop="10dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/imageView2"
                app:srcCompat="@drawable/mountainclimbers" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:paddingLeft="10dp"
                android:text="Mountain Climbers"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView4"
                app:layout_constraintTop_toBottomOf="@id/textView4" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="50dp"
                android:text="20 reps"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView4"
                app:layout_constraintTop_toBottomOf="@id/textView5" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="166dp"
                android:layout_marginTop="10dp"
                android:layout_height="90dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/imageView4"
                app:srcCompat="@drawable/vup" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:paddingLeft="60dp"
                android:text="V-Up"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView6"
                app:layout_constraintTop_toBottomOf="@id/textView6" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="60dp"
                android:text="15 reps"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView6"
                app:layout_constraintTop_toBottomOf="@id/textView7" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="172dp"
                android:layout_height="75dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/imageView6"
                app:srcCompat="@drawable/bicyclecrunch" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:paddingLeft="30dp"
                android:text="Bicycle Crunches"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView7"
                app:layout_constraintTop_toBottomOf="@id/textView8" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="60dp"
                android:text="20 reps"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView7"
                app:layout_constraintTop_toBottomOf="@id/textView9" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="178dp"
                android:layout_height="77dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/imageView7"
                app:srcCompat="@drawable/russiantwist" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:paddingLeft="30dp"
                android:text="Russian Twist"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView8"
                app:layout_constraintTop_toBottomOf="@id/textView10" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="50dp"
                android:text="25 reps"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView8"
                app:layout_constraintTop_toBottomOf="@id/textView11" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="220dp"
                android:layout_height="120dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/imageView8"
                app:srcCompat="@drawable/heeltouch" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:paddingLeft="20dp"
                android:text="Heel Touch"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView9"
                app:layout_constraintTop_toBottomOf="@id/textView12" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="50dp"
                android:text="30 reps"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="italic"
                app:layout_constraintLeft_toRightOf="@id/imageView9"
                app:layout_constraintTop_toBottomOf="@id/textView13" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>


